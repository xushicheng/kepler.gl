{
  "author": "Shan He <shan@uber.com>",
  "dependencies": {
    "@kepler.gl/types": "3.0.0-alpha.0",
    "@types/d3-scale": "^3.2.2",
    "@types/keymirror": "^0.1.1",
    "colorbrewer": "^1.5.0",
    "d3-scale": "^3.2.3",
    "global": "^4.3.0",
    "keymirror": "^0.1.1"
  },
  "description": "kepler.gl constants used by kepler.gl components, actions and reducers",
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "dist",
    "umd"
  ],
  "keywords": [
    "babel",
    "es6",
    "react",
    "webgl",
    "visualization",
    "deck.gl"
  ],
  "license": "MIT",
  "main": "src/index.ts",
  "maintainers": [
    "Shan He <heshan0131@gmail.com>",
    "Giuseppe Macri <gmacri@uber.com>"
  ],
  "name": "@kepler.gl/constants",
  "nyc": {
    "instrument": false,
    "sourceMap": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keplergl/kepler.gl.git"
  },
  "scripts": {
    "build": "rm -fr dist && babel src --out-dir dist --source-maps inline --extensions '.ts,.tsx,.js,.jsx' --ignore '**/*.d.ts'",
